                maxWidth -= 78;
                sletat.FrameSearch.$create({
                    city: 1264,
                    country: 113,
                    namespace: "macstyle",
                    formType: "horizontal",
                    useCard: false,
                    customStyles: "#search-form .column-wrapper { background: transparent; box-shadow: none; border: none;" +
                    " } #search-form p { color: white; } #submit-decor { text-align: center !important; }" +
                    "#sletat-search-1 { min-width: 100% !important; }" +
                    "@media (max-width: 768px) { body { max-width: " + maxWidth + "px !important;} .type-horizontal .column" +
