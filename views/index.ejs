                    style="margin: 0 auto; background:url('http://ui.sletat.ru/gfx/ld3.gif') no-repeat center center;"
                    width="591" height="487" frameborder="0" scrolling="no"></iframe>
            <iframe allowtransparency="true" onload="sm2_sly_4iutv.init()" id="sm2_slyResult_4iutv"
                    src="http://ui.sletat.ru/HotResult.html?sfx=_4iutv&fbg=ffffff&mbg=-1&bbg=-1&bbd=eeeeee&cbd=abadb3&c1=222222&c2=838383&c3=9d1414&c4=ededed&style=.sqrd.country%20span%23main%7Bfont-weight%3A%20normal%20!important%3B%7D.sqrd.country%20span%23main%7Bfont-style%3A%20italic%20!important%3B%7D.sqrd.country%7B-moz-border-radius%3A5px%20!important%3Bborder-radius%3A%205px%20!important%3B%7D.sqrd.country%20span%23main%7Bfont-family%3A%20Verdana%2C%20Geneva%2C%20sans-serif%20!important%3B%7D&tpl=sqrd&currency=RUB&rc=5&stpl=%D0%93%D0%BE%D1%80%D1%8F%D1%89%D0%B8%D0%B5%20%D1%82%D1%83%D1%80%D1%8B%20(%D0%A1%D0%9F%D0%B1)&settings={'plugins':['squared_output']}"
